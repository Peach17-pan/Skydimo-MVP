{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${config:idf.toolsPathWin}\\tools\\xtensa-esp-elf\\esp-14.2.0_20250730\\xtensa-esp-elf\\bin\\xtensa-esp32s3-elf-gcc.exe",
      "compileCommands": [
        "${config:idf.buildPath}/compile_commands.json"
      ],
      "includePath": [
        "${config:idf.espIdfPath}/components/**",
        "${config:idf.espIdfPathWin}/components/**",
        "${workspaceFolder}/**",
        "${env:IDF_PATH}/components/**",
        "${env:IDF_PATH}/tools/esp-rom-elfs/esp32s3/**",
        "${workspaceFolder}/**",
        "${workspaceFolder}/build/config/**"
      ],
      "browse": {
        "path": [
          "${config:idf.espIdfPath}/components",
          "${config:idf.espIdfPathWin}/components",
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true
      },
      "intelliSenseMode": "linux-gcc-x64"
    }
  ],
  "version": 4
}
